# WiFi DensePose Service Environment Variables

# Service Settings
WIFI_POSE_HOST=0.0.0.0
WIFI_POSE_PORT=8001
WIFI_POSE_DEBUG=false

# Domain (healthcare, fitness, smart_home, security)
WIFI_POSE_DOMAIN=healthcare

# Mock Mode (true for demo without hardware)
WIFI_POSE_USE_MOCK_DATA=true
WIFI_POSE_MOCK_PERSON_COUNT=3

# Frame Rate
WIFI_POSE_FRAME_RATE=30

# Analytics
WIFI_POSE_FALL_DETECTION_ENABLED=true
WIFI_POSE_FALL_DETECTION_THRESHOLD=0.8
WIFI_POSE_ACTIVITY_RECOGNITION_ENABLED=true

# Backend Integration
WIFI_POSE_BACKEND_URL=http://localhost:8080

# Redis
WIFI_POSE_REDIS_URL=redis://localhost:6379

# Logging
WIFI_POSE_LOG_LEVEL=INFO
